# gas-station/.env.example

# Port for the gas station server
PORT=5000

# Sui RPC endpoint
SUI_RPC_URL=https://fullnode.testnet.sui.io:443

# Sponsor wallet private key (base64 encoded)
# Generate with: sui keytool generate ed25519
# Then export: sui keytool export --key-identity <alias>
SPONSOR_PRIVATE_KEY=your_base64_encoded_private_key_here
